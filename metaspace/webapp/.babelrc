{
  "env": {
    "test": {
      "presets": [
        ["env", {"modules": "commonjs"}]
      ]
    }
  },
  "presets": [
    ["env", {"modules": false}]
  ],
  "plugins": [
    ["component", [{
      "libraryName": "element-ui",
      "styleLibraryName": "theme-default"
    }]],

    "transform-object-rest-spread",
    "transform-runtime",
    "syntax-dynamic-import"
  ]
}
