{
  "name": "sm-webapp-ng",
  "description": "Metaspace web application",
  "version": "1.0.1",
  "author": "Alexandrov Team, EMBL",
  "contributors": [
    "Andrew Palmer <andrew.palmer@embl.de>",
    "Artem Tarasov <artem.tarasov@embl.de>",
    "Ivan Protsyuk <ivan.protsyuk@embl.de>",
    "Lachlan Stuart <lachlan.stuart@embl.de>",
    "Renat Nigmetzianov <renat.nigmetzianov@embl.de>",
    "Theodore Alexandrov <theodore.alexandrov@embl.de>",
    "Vitaly Kovalev <vitaly.kovalev@embl.de>"
  ],
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development vue-cli-service serve",
    "test": "yarn run generate-local-graphql-schema && cross-env NODE_ENV=test jest",
    "test-ci": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "deref-schema": "node ./deref_schema.js src/lib/metadataSchemas",
    "generate-local-graphql-schema": "cd ../graphql && yarn run gen-graphql-schema",
    "fetch-prod-graphql-schema": "apollo schema:download tests/utils/graphql-schema.json --endpoint https://metaspace2020.eu/graphql",
    "build": "yarn run deref-schema && cross-env NODE_ENV=production vue-cli-service build --modern",
    "build-ci": "yarn run deref-schema && cross-env NODE_ENV=production vue-cli-service build --no-progress --hide-modules",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/js-cookie": "^2.1.0",
    "@types/marked": "^0.6.2",
    "@types/sanitize-html": "^1.18.2",
    "@types/upng-js": "^2.1.1",
    "@types/ws": "^5.1.2",
    "@vue/cli": "^3.11.0",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-batch-http": "^1.2.12",
    "core-js": "^3.4.4",
    "d3": "^5.11.0",
    "date-fns": "^1.29.0",
    "dom-to-image-google-font-issue": "^2.6.2",
    "element-ui": "^2.12.0",
    "file-saver": "^1.3.3",
    "fine-uploader": "^5.13.0",
    "graphql": "^14.5.3",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.10",
    "marked": "^0.6.1",
    "numeric": "^1.2.6",
    "plotly.js": "^1.34.0",
    "popper.js": "^1.14.7",
    "raven-js": "^3.25.2",
    "sanitize-html": "^1.20.0",
    "simple-statistics": "^7.0.2",
    "subscriptions-transport-ws": "^0.9.16",
    "ts-essentials": "3.0.4",
    "upng-js": "https://github.com/photopea/UPNG.js.git#f550095",
    "uuid": "^3.0.1",
    "vue": "^2.6.11",
    "vue-analytics": "^5.9.1",
    "vue-apollo": "^3.0.0-rc.2",
    "vue-class-component": "^7.1.0",
    "vue-clickaway": "^2.1.0",
    "vue-lazyload": "^1.0.0-rc5",
    "vue-property-decorator": "^8.2.2",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "3.0.1",
    "vue-slide-up-down": "^1.3.3",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/file-saver": "0.0.1",
    "@types/jest": "^24.0.18",
    "@types/lodash-es": "~4.17.0",
    "@types/plotly.js": "1.28.11",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "apollo-link-schema": "^1.2.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "frontmatter-markdown-loader": "^1.8.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "json-schema-deref-sync": "^0.10.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "ts-jest": "^24.3.0",
    "typescript": "3.6.5",
    "vue-cli-plugin-apollo": "^0.21.3",
    "vue-template-compiler": "2.6.11",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "lodash-es": "lodash",
      "\\.scss$": "<rootDir>/tests/utils/scssMock.js",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/utils/setupTestFramework.ts"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    },
    "clearMocks": true,
    "restoreMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "vue"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js,vue}",
      "!src/**/*.{test,spec,d}.{ts,js}"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "browserslist": {
    "production": [
      "defaults"
    ]
  },
  "_eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
