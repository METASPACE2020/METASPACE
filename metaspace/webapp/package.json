{
  "name": "sm-webapp-ng",
  "description": "Metaspace web application",
  "version": "1.0.1",
  "author": "Artem Tarasov <artem.tarasov@embl.de>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --max_old_space_size=4096 server.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-ci": "cross-env NODE_ENV=production webpack --hide-modules",
    "test": "yarn run generate-local-graphql-schema && cross-env NODE_ENV=test jest",
    "test-ci": "cross-env NODE_ENV=test jest",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "deref-schema": "node ./deref_schema.js src/lib/metadataSchemas",
    "generate-local-graphql-schema": "cd ../graphql && yarn run gen-graphql-schema",
    "fetch-prod-graphql-schema": "apollo schema:download tests/utils/graphql-schema.json --endpoint https://metaspace2020.eu/graphql"
  },
  "dependencies": {
    "@types/js-cookie": "^2.1.0",
    "@types/marked": "^0.6.2",
    "@types/sanitize-html": "^1.18.2",
    "@types/upng-js": "^2.1.1",
    "@types/ws": "^5.1.2",
    "apollo-client": "^2.3.7",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.1",
    "apollo-link-ws": "^1.0.8",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "concat-files": "^0.1.1",
    "connect-history-api-fallback": "^1.5.0",
    "d3": "4.10.1",
    "d3-color": "^1.0.3",
    "d3-scale": "^1.0.5",
    "d3-svg-legend": "^2.24.0",
    "date-fns": "^1.29.0",
    "deepcopy": "^0.6.3",
    "dom-to-image-google-font-issue": "^2.6.2",
    "element-ui": "2.4.5",
    "express": "^4.14.0",
    "file-saver": "^1.3.3",
    "fine-uploader": "^5.13.0",
    "fork-ts-checker-webpack-plugin": "^1.4.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "js-cookie": "^2.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.10",
    "marked": "^0.6.1",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "numeric": "^1.2.6",
    "plotly.js": "^1.34.0",
    "popper.js": "^1.14.7",
    "raven": "^2.6.2",
    "raven-js": "^3.25.2",
    "sanitize-html": "^1.20.0",
    "serve-favicon": "^2.3.2",
    "simple-statistics": "^7.0.2",
    "subscriptions-transport-ws": "^0.9.14",
    "ts-loader": "^4.3.0",
    "upng-js": "https://github.com/photopea/UPNG.js.git#f550095",
    "uuid": "^3.0.1",
    "vue": "~2.5.16",
    "vue-analytics": "^5.9.1",
    "vue-apollo": "3.0.0-beta.13",
    "vue-clickaway": "^2.1.0",
    "vue-lazyload": "^1.0.0-rc5",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.0.1",
    "vue-slide-up-down": "^1.3.3",
    "vuex": "~3.0.1",
    "vuex-router-sync": "^5.0.0",
    "zen-observable-ts": "^0.8.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/cucumber": "2.0.4",
    "@types/d3": "4.10.1",
    "@types/file-saver": "0.0.1",
    "@types/jest": "^23.1.5",
    "@types/lodash-es": "~4.17.0",
    "@types/node": "8.0.31",
    "@types/node-fetch": "1.6.7",
    "@types/plotly.js": "1.28.11",
    "@types/webpack-env": "1.13.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "apollo": "^1.7.0",
    "apollo-link-schema": "^1.1.0",
    "autoprefixer": "^6.7.2",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "frontmatter-markdown-loader": "^1.8.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "graphql-tools": "^3.1.1",
    "html-loader": "^0.5.5",
    "jest": "^24.0.0",
    "jest-serializer-vue": "^2.0.2",
    "json-schema-deref-sync": "^0.10.1",
    "node-sass": "4.5.3",
    "postcss-loader": "~2.1.5",
    "sass-loader": "7.0.1",
    "source-map-loader": "0.2.3",
    "style-loader": "~0.21.0",
    "supertest": "^3.4.2",
    "ts-jest": "^23.1.4",
    "ts-node": "~3.1.0",
    "typescript": "~2.8.4",
    "vue-class-component": "6.2.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "~15.2.2",
    "vue-property-decorator": "6.1.0",
    "vue-template-compiler": "~2.5.16",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.11",
    "webpack-hot-middleware": "^2.22.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "transform": {
      "^.+\\.vue$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.[jt]sx?$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleNameMapper": {
      "lodash-es": "lodash",
      "\\.scss$": "<rootDir>/tests/utils/scssMock.js",
      "\\.(png|jpg|gif|svg)$": "<rootDir>/tests/utils/staticFileMock.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/utils/setupTestFramework.ts"
    ],
    "clearMocks": true,
    "restoreMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "vue"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js,vue}",
      "!src/**/*.{test,spec,d}.{ts,js}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "browserslist": {
    "production": [
      "defaults"
    ]
  }
}
